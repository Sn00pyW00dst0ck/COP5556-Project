program Arithmetic_Operators;
begin
  writeln (3 + 4);
  writeln (6 + 9.5);
  writeln ('Hello World' + '!');
  writeln ('Hello World' + CHR(60));
  writeln (CHR(65) + CHR(66));
  
  writeln (3 - 4);
  writeln (3 - 4.5);
  
  writeln (3 * 4);
  writeln (3 * 4.5);
  
  writeln (4 / 2);
  writeln (6.0 / 1.5);

  writeln ( -5 );
end.
